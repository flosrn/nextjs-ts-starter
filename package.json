{
  "name": "nextjs-ts-starter",
  "description": "A Next.js starter project with TypeScript and TailwindCSS",
  "version": "0.1.3",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepare": "husky install",
    "postbuild": "next-sitemap",
    "release": "standard-version",
    "packages:upgrade": "yarn upgrade-interactive --latest",
    "git:commit": "yarn lint:fix && git add -A && yarn cz",
    "git:push": "git push --follow-tags",
    "expansion-pack:add": "./expansion-pack.sh"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^2.0.11",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.1.11",
    "framer-motion": "^7.3.6",
    "next": "^12.3.1",
    "next-auth": "^4.11.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.4.0",
    "swr": "^1.3.0",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@svgr/webpack": "^6.3.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.21",
    "@types/tailwindcss": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "10.4.12",
    "babel-jest": "^29.1.0",
    "commitizen": "^4.2.5",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-customizable": "^7.0.0",
    "eslint": "8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.0",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.22",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-test-renderer": "^18.2.0",
    "standard-version": "^9.5.0",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
