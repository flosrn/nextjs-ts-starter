{
  "name": "nextjs-ts-starter",
  "description": "A Next.js starter project with TypeScript ans TailwindCSS",
  "version": "0.1.3",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepare": "husky install",
    "postbuild": "next-sitemap",
    "release": "standard-version",
    "packages:upgrade": "yarn upgrade-interactive --latest",
    "git:commit": "git add -A && yarn cz",
    "git:push": "git push --follow-tags",
    "expansion-pack:add": "./expansion-pack.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook:generate": "yarn plop"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "classnames": "^2.3.1",
    "next": "^12.0.8",
    "next-themes": "^0.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/bundle-analyzer": "^12.0.8",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@storybook/theming": "^6.4.13",
    "@svgr/webpack": "^6.2.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.38",
    "@types/tailwindcss": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "inquirer-fuzzy-path": "^2.3.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "next-sitemap": "^1.9.9",
    "plop": "^3.0.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-sort-class-names": "^3.0.1",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.2",
    "storybook-theme-css-vars": "^0.0.4",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "resolutions": {
    "webpack": "^5"
  }
}
